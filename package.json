{
  "name": "ace-physics",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "validate": "jest lib/tutorial/validate-tutorials.test.ts",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "chrome-debug": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222",
    "build": "npm run validate && next build",
    "start": "next start",
    "script": "ts-node -r tsconfig-paths/register --transpile-only --script-mode scripts/run.ts",
    "dynamodb-setup": "docker create --name dynamodb -p 8000:8000 amazon/dynamodb-local",
    "dynamodb-start": "docker restart dynamodb; sleep 3; node ./scripts/create-dynamodb-table.js",
    "dynamodb-stop": "docker stop dynamodb"
  },
  "engines": {
    "npm": "^10.0.0",
    "node": "^20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.261.0",
    "@aws-sdk/client-sesv2": "^3.261.0",
    "@aws-sdk/lib-dynamodb": "^3.261.0",
    "@emotion/is-prop-valid": "^1.2.0",
    "@next-auth/dynamodb-adapter": "^1.2.0",
    "@next/eslint-plugin-next": "^13.1.6",
    "@next/font": "^13.1.6",
    "@primer/octicons-react": "^17.11.1",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/nextjs": "^7.34.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/katex": "^0.16.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "csv": "^6.2.5",
    "downshift": "^7.2.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "katex": "^0.16.4",
    "lodash.debounce": "^4.0.8",
    "next": "^13.1.6",
    "next-auth": "^4.19.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "swr": "^2.0.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
