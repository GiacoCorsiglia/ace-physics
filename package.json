{
  "name": "ace-physics",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "validate": "jest lib/tutorial/validate-tutorials.test.ts",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "chrome-debug": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222",
    "build": "npm run validate && next build",
    "start": "next start",
    "script": "ts-node -r tsconfig-paths/register --transpile-only --script-mode scripts/run.ts",
    "dynamodb-setup": "docker create --name dynamodb -p 8000:8000 amazon/dynamodb-local",
    "dynamodb-start": "docker restart dynamodb; sleep 3; node ./scripts/create-dynamodb-table.js",
    "dynamodb-stop": "docker stop dynamodb"
  },
  "engines": {
    "npm": ">8.0.0",
    "node": "^14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.44.0",
    "@aws-sdk/client-sesv2": "^3.43.0",
    "@aws-sdk/lib-dynamodb": "^3.44.0",
    "@emotion/is-prop-valid": "^1.1.0",
    "@next-auth/dynamodb-adapter": "^1.0.1",
    "@next/eslint-plugin-next": "^12.0.0",
    "@primer/octicons-react": "^16.2.0",
    "@sentry/nextjs": "^6.16.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/katex": "^0.11.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "downshift": "^6.1.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.4.5",
    "katex": "^0.13.19",
    "lodash.debounce": "^4.0.8",
    "next": "^12.0.0",
    "next-auth": "^4.0.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.45.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "csv": "^5.3.2",
    "dotenv": "^10.0.0",
    "sqlite": "^4.0.17",
    "sqlite3": "5.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0"
  }
}
