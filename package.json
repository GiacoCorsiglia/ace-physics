{
  "name": "ace-physics",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "validate": "jest lib/tutorial/validate-tutorials.test.ts",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "chrome-debug": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222",
    "build": "npm run validate && next build",
    "start": "next start",
    "script": "ts-node -r tsconfig-paths/register --transpile-only --script-mode scripts/run.ts",
    "dynamodb-setup": "docker create --name dynamodb -p 8000:8000 amazon/dynamodb-local",
    "dynamodb-start": "docker restart dynamodb; sleep 3; node ./scripts/create-dynamodb-table.js",
    "dynamodb-stop": "docker stop dynamodb"
  },
  "engines": {
    "npm": ">8.0.0",
    "node": "^14.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.178.0",
    "@aws-sdk/client-sesv2": "^3.178.0",
    "@aws-sdk/lib-dynamodb": "^3.178.0",
    "@emotion/is-prop-valid": "^1.2.0",
    "@next-auth/dynamodb-adapter": "^1.0.4",
    "@next/eslint-plugin-next": "^12.3.1",
    "@primer/octicons-react": "^17.5.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/nextjs": "^6.19.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^29.0.3",
    "@types/katex": "^0.14.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^14.18.30",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "csv": "^6.2.0",
    "downshift": "^6.1.11",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "katex": "^0.16.2",
    "lodash.debounce": "^4.0.8",
    "next": "^12.3.1",
    "next-auth": "^4.10.3",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.55.0",
    "swr": "^1.3.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "dotenv": "^16.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0"
  }
}
