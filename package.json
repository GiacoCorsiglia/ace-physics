{
  "name": "ace-physics",
  "private": true,
  "license": "Polyform-Noncommercial-1.0.0",
  "version": "0.0.0",
  "scripts": {
    "test": "vitest",
    "validate": "vitest lib/tutorial/validate-tutorials.test.ts",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "chrome-debug": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222",
    "build": "npm run validate && next build",
    "start": "next start",
    "script": "ts-node -r tsconfig-paths/register --transpile-only --script-mode scripts/run.ts",
    "dynamodb-setup": "docker create --name dynamodb -p 8000:8000 amazon/dynamodb-local",
    "dynamodb-start": "docker restart dynamodb; node ./scripts/wait-for-dynamodb.js; node ./scripts/create-dynamodb-table.js",
    "dynamodb-stop": "docker stop dynamodb"
  },
  "engines": {
    "npm": "^10.0.0",
    "node": "^20.10.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/client-sesv2": "^3.849.0",
    "@aws-sdk/lib-dynamodb": "^3.848.0",
    "@emotion/is-prop-valid": "^1.2.2",
    "@next-auth/dynamodb-adapter": "^3.0.2",
    "@next/eslint-plugin-next": "^14.1.4",
    "@primer/octicons-react": "^19.9.0",
    "@sentry/nextjs": "^7.109.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/katex": "^0.16.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "csv": "^6.3.8",
    "downshift": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "katex": "^0.16.10",
    "lodash.debounce": "^4.0.8",
    "next": "^14.1.4",
    "next-auth": "^4.24.7",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.72.0",
    "swr": "^2.2.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
